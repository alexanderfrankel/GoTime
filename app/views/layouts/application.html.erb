<!DOCTYPE html>
<html>
<head>
  <title>TimeFly</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div>
		<% if current_user %>
			<%= link_to "Sign Out", signout_path %>
		<% else %>
			<%= link_to "Sign In", "/auth/google_oauth2" %>
		<% end %>
	</div>

	<div>
		<%= yield %>
	</div>

</body>
</html>
